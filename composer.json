{
    "name": "joren/acf-builder",
    "description": "A simple PHP api for creating ACF Custom Field Groups",
    "type": "library",
    "license": "MIT",
    "version": "3.0.5",
    "autoload": {
        "psr-4": {
            "Joren\\ACFBuilder\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Joren Rothman",
            "email": "rothmanjoren@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit --colors",
        "publish": "vendor/bin/phabel publish && git push origin main && git push origin --tags"
    },
    "minimum-stability": "alpha",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phabel/phabel": "^1"
    },
    "config": {
        "allow-plugins": {
            "phabel/phabel": true
        }
    },
    "extra": {
        "phabel": {
            "revision": 0
        }
    }
}
